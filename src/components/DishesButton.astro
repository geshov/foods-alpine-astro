<div class="indicator flex-grow flex-shrink pt-1">
  <span x-text="count.dishes" class="indicator-item badge font-medium me-6">
  </span>
  <button
    @click="view = 'dishes'"
    class="btn btn-square btn-outline w-full"
    :class="view ==='dishes' && 'btn-active'"
